    <body>

        <main>
            <div class="container">

                <section class="section register min-vh-100 d-flex flex-column align-items-center justify-content-center py-4">
                    <div class="container">
                        <div class="row justify-content-center">
                            <div class="col-lg-4 col-md-6 d-flex flex-column align-items-center justify-content-center">

                                <div class="d-flex justify-content-center py-4">
                                    <a href="index.html" class="logo d-flex align-items-center w-auto">
                                        <img src="/Content/AdminLayout/assets/img/logo.png" alt="">
                                        <span class="d-none d-lg-block">HALLEDERİZ</span>
                                    </a>
                                </div> 

                                <div class="card mb-3">

                                    <div class="card-body">

                                        <div class="pt-4 pb-2">
                                            <h5 class="card-title text-center pb-0 fs-4">Kayıt Ol</h5>

                                        </div>
                                        @using (Html.BeginForm("Afisolustur", "Default"))
                                        {
                                    <form name="form" class="row g-3 needs-validation" novalidate>
                                        <div class="col-12">
                                            <label for="isim" class="form-label">Adınız</label>
                                            <input type="text" name="isim" class="form-control" id="isim" required>
                                            <div class="invalid-feedback">Lütfen, Adınızı Giriniz!</div>
                                        </div>

                                        <div class="col-12">

                                            <label for="soyisim" class="form-label">Soyadınız</label>
                                            <input type="text" name="soyisim" class="form-control" id="soyisim" required>
                                            <div class="invalid-feedback">Lütfen, Soyadınızı Giriniz!</div>
                                        </div>

                                        <div class="col-12">
                                            <label for="kullaniciadi" class="form-label">Kullanıcı Adı</label>
                                            <div class="input-group has-validation">
                                                <span class="input-group-text" id="inputGroupPrepend"></span>
                                                <input type="text" name="kullaniciadi" class="form-control" id="kullaniciadi" required>
                                                <div class="invalid-feedback">Kullanıcı Adı Giriniz..</div>
                                            </div>
                                        </div>

                                        <div class="col-12">
                                            <label for="sifre" class="form-label">Şifre</label>
                                            <input type="password" name="sifre" class="form-control" id="sifre" required>
                                            <div class="invalid-feedback">Lütfen, Şifrenizi Giriniz!</div>
                                        </div>

                                        <div class="col-12">
                                            <label for="email" class="form-label">E-mail</label>
                                            <div class="input-group has-validation">
                                                <span class="input-group-text" id="inputGroupPrepend"></span>
                                                <input type="email" name="email" class="form-control" id="email" required>
                                                <div class="invalid-feedback">E-mail Adresinizi Giriniz..</div>
                                            </div>
                                        </div>

                                        <div class="col-12">
                                            <label for="telefon" class="form-label">Telefon</label>

                                            <div class="input-group has-validation">

                                                <input type="text" name="telefon" class="form-control" id="telefon" required>
                                                <div class="invalid-feedback">Telefon Numaranızı Giriniz..</div>
                                            </div>
                                        </div>

                                        <div class="col-12">
                                            <label for="il" class="form-label">İl</label>
                                            <div class="input-group has-validation">

                                                <input type="text" name="il" class="form-control" id="İl" required>
                                                <div class="invalid-feedback">İl Adı Giriniz..</div>
                                            </div>
                                        </div>

                                        <div class="col-12">
                                            <label for="ilce" class="form-label">İlçe</label>
                                            <div class="input-group has-validation">

                                                <input type="text" name="ilce" class="form-control" id="ilce" required>
                                                <div class="invalid-feedback">İlçe Adı Giriniz..</div>
                                            </div>
                                        </div>


                                        <div class="col-12">
                                            <button class="btn btn-primary w-100" type="submit">Üye Ol</button>
                                        </div>
                                        <div class="d-none" id="submitSuccessMessage">
                                            <div class="text-center text-dark mb-3">
                                                <div class="fw-bolder">Kayıt başarılı!</div>

                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <p class="small mb-0">Zaten kayıtlı mısınız? <a href="~/Content/AdminLayout/pages-login.html">Giriş Yap</a></p>
                                        </div>
                                    </form>
                                        }
                                    </div>
                                </div>



                            </div>
                        </div>
                    </div>

                </section>

            </div>
        </main> 

        <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

        
        <script src="/Content/AdminLayout/assets/vendor/apexcharts/apexcharts.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/chart.js/chart.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/echarts/echarts.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/quill/quill.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/simple-datatables/simple-datatables.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/tinymce/tinymce.min.js"></script>
        <script src="/Content/AdminLayout/assets/vendor/php-email-form/validate.js"></script>

       
        <script src="/Content/AdminLayout/assets/js/main.js"></script>

    </body>




